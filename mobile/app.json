{
  "expo": {
    "name": "Навчальна система",
    "slug": "learning-system",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "learningsystem",
    "userInterfaceStyle": "automatic",
    "splash": {
      "backgroundColor": "#ffffff",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.learningsystem.app",
      "infoPlist": {
        "NSFaceIDUsageDescription": "Використовується для безпечного входу в додаток",
        "UIRequiresFullScreen": false,
        "UIViewControllerBasedStatusBarAppearance": true
      }
    },
    "android": {
      "package": "com.learningsystem.app",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ]
    },
    "plugins": [
      "expo-dev-client",
      [
        "expo-notifications",
        {
          "color": "#ffffff"
        }
      ],
      [
        "expo-local-authentication"
      ],
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "a17a8758-b1f8-4a8a-9990-d4bd3da8a45c"
      }
    },
    "owner": "kultup"
  }
}
